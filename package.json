    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.26",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.34",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
